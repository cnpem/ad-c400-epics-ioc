record(ai, "$(P)$(R)get_DAC_ch1")
{
   field(DESC,"Get DAC for ch1")
   field(PINI, "1")
   field(DTYP, "asynFloat64")
   field(INP,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))DAC1")
   field(PREC, "5")
   field(SCAN, "I/O Intr")
   field(EGU,"V")
}

